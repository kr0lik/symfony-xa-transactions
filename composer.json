{
  "name": "kr0lik/symfony-xa-transactions",
  "type": "library",
  "description": "Mysql XA Transaction for Symfony",
  "keywords": ["symfony","mysql","transaction","xa-transaction","symfony4"],
  "license": "MIT",
  "support": {
    "source": "https://github.com/kr0lik/symfony-xa-transactions"
  },
  "authors": [
    {
      "name": "krolik",
      "email": "krolik@yandex.ru"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "bin-dir": "bin",
    "secure-http": false
  },
  "autoload": {
    "psr-4": {
      "kr0lik\\XATransaction\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "kr0lik\\XATransaction\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1.3",
    "symfony/orm-pack": "^1.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.11.5"
  }
}
